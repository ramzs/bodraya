            p.history__gazeta-title Белгородская Правда 19.01.1973
        .history__gazeta-item.history__gazeta-item--2
          +picture("history", "gazeta-2", "png").history__image-box-img.history__gazeta-img.history__gazeta-img--2
          .history__gazeta-footer.history__gazeta-footer--2
            img(src="img/history/gazeta-bg-2.svg" alt="").history__gazeta-bg.history__gazeta-bg--2
            p.history__gazeta-title Белгородская Правда 02.07.1972
        .history__gazeta-item.history__gazeta-item--3
          +picture("history", "gazeta-3", "png").history__image-box-img.history__gazeta-img.history__gazeta-img--3
