  +picture("history", "bg-2-m", "png").history__bg.history__bg--1.for-mobile
  +picture("history", "bg-2-t", "png").history__bg.history__bg--1.for-tablet
  +picture("history", "bg-2", "png").history__bg.history__bg--1.for-desktop
